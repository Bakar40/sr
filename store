    <script>
        jQuery(function () {
            var countdowns = $(".product-price");
            countdowns.each(function () {
                setInterval(function (ele) {
                    timeLeft = parseInt(ele.attr("data-countdown"));
                    if (timeLeft == 0) {
                        ele.html("Time Expired!");
                    } else {
                        timeLeft--;
                        ele.attr("data-countdown", timeLeft);
                        var days = Math.floor(timeLeft / (60 * 60 * 24));
                        var hours = Math.floor((timeLeft % (60 * 60 * 24)) / (60 * 60));
                        var minutes = Math.floor((timeLeft % (60 * 60)) / (60));
                        var seconds = Math.floor((timeLeft % 60));
                        ele.html(days + "d " + hours + "h " + minutes + "m " + seconds + "s");
                    }
                }, 1000, jQuery(this));
            });
        });








        // Array to store generated IDs
        let usedIds = [];

        // Function to generate random ID
        function generateId() {
            const alphabet = "abcdefghijklmnopqrstuvwxyz0123456789";
            let id = "";
            for (let i = 0; i < 8; i++) {
                id += alphabet[Math.floor(Math.random() * alphabet.length)];
            }
            // Check if ID has been generated before
            if (usedIds.includes(id)) {
                return generateId();
            } else {
                usedIds.push(id);
                return id;
            }
        }

        // Function to copy ID to clipboard
        function copyToClipboard(text) {
            const input = document.createElement("textarea");
            input.value = text;
            document.body.appendChild(input);
            input.select();
            document.execCommand("copy");
            document.body.removeChild(input);
        }

        // Get all elements with class "copy-text"
        const elements = document.getElementsByClassName("copy-text");

        // Loop through each element
        for (let i = 0; i < elements.length; i++) {
          
            
            // Add a click event listener to the element
            elements[i].addEventListener("click", function () {
                // Get the text inside the span
                const text = this.getElementsByClassName("random-id")[0].innerText;
                // Copy the text to the clipboard
                copyToClipboard(text);
                // Log a message to the console to indicate that the text has been copied
                console.log("Product ID copied to clipboard: " + text);
            });
        }
    </script>

