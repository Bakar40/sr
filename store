        src="/template-assets/store.js?updated=076aa082061bc258c913ecf77ff915e148bda3b44d8e0220ec590b519530d036"></script>
    <script
        src="/template-assets/ihatetebexsomuch.js?updated=076aa082061bc258c913ecf77ff915e148bda3b44d8e0220ec590b519530d036"></script>
